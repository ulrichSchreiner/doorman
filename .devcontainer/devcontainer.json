{
    "name": "doorman",
    "dockerComposeFile": [
        "${env:HOME}/.config/vscode/go-compose.yml"
    ],
    "workspaceFolder": "${localWorkspaceFolder}",
    "service": "go-dev-tools",
    "extensions": [
        "golang.go",
        "humao.rest-client",
        "dline.cobaltnext",
        "sirtori.indenticator",
        "wayou.vscode-todo-highlight",
        "gruntfuggly.todo-tree",
        "eamodio.gitlens",
        "compulim.vscode-clock"
    ]
}